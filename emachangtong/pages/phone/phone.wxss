/* pages/phone/phone.wxss */
.container{
  padding: 0;
}
.title{
  width: 100%;
  height: 400rpx;
  color: #ffffff;
  background-color: #3a82ff;
}
.title-list{
  width: 400rpx;
  height: 200rpx;
  font-size: 24px;
  font-weight: 500;
  margin: 0 auto;
  line-height: 200rpx;
}
.item-left{
  float: left;
}
.item-right{
  float: right;
}
.title-bottom{
  width: 200rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  margin: 0 auto;
  font-size: 16px;
  border: 1px solid #ffffff;
}
.call{
  width: 80%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  color: #ffffff;
  background-color: #3a82ff;
  margin-top: 50rpx;
  border-radius: 10rpx;
}